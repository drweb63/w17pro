<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  
    <bool name="config_unplugTurnsOnScreen">true</bool>

    <bool name="config_allowAllRotations">true</bool>

    <bool name="config_batterySdCardAccessibility">true</bool>

    <bool name="config_enableLockScreenRotation">true</bool>

    <string name="config_networkLocationProviderPackageName">com.google.android.location</string>

    <string name="config_geocodeProviderPackageName">com.google.android.location</string>

    <bool name="config_automatic_brightness_available">false</bool>

    <bool name="config_voice_capable">false</bool>

    <bool name="config_sms_capable">true</bool>
    
    <bool name="config_showNavigationBar">true</bool>
   
    <bool translatable="false" name="config_wifi_dual_band_support">false</bool>
    
    <bool translatable="false" name="config_wifi_background_scan_support">true</bool>
    
    <bool translatable="false" name="config_wifi_p2p_support">false</bool>
    
    <integer name="config_screenBrightnessDim">30</integer>
    
    <integer name="db_connection_pool_size">3</integer>
    
    <integer name="config_cursorWindowSize">2048</integer>
    
    <integer name="config_lowBatteryCloseWarningLevel">15</integer>
    
    <integer name="config_lowBatteryWarningLevel">10</integer>
    
    <string-array translatable="false" name="networkAttributes">
	<item>"wifi,1,1,1,-1,true"</item>
	<item>"mobile,0,0,0,-1,true"</item>
	<item>"wifi_p2p,13,1,0,-1,true"</item>
	<item>"ethernet,9,9,1,-1,true"</item>
    </string-array>
    
    <string-array translatable="false" name="radioAttributes">
        <item>"1,1"</item>
        <item>"0,1"</item>
        <item>"9,1"</item>
    </string-array>
    
    <integer-array translatable="false" name="config_tether_upstream_types">
        <item>0</item>
        <item>9</item>
    </integer-array>
    
    <string translatable="false" name="config_ethernet_iface_regex">eth\\d|usbnet\\d</string>
</resources>
